<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wu.greenfarm.mapper.ProductOrderMapper">

    <insert id="addProductOrder" parameterType="ProductOrder">
        insert into t_product_order(product_id,customer_id,farmer_id,amount,money,status,address)
        value (#{productId},#{customerId},#{farmerId},#{amount},#{money},#{status},#{address})
    </insert>


</mapper>